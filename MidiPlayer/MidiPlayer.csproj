  </ItemGroup>

  <ItemGroup>
    <Content Include="assets\Mozart-RondoAllaTurca.mid">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
